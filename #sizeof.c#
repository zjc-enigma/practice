#include<stdio.h>

int main() {
  int a[10] = {3};
  printf("%d\n", a[5]);
  int * b = (int*)malloc(sizeof(int)*10);
  printf("%d\n", sizeof(a));
  printf("%d\n", sizeof(b));
  printf("%lu\n", sizeof(*a));
  printf("%d\n", sizeof(char*));
  printf("%d\n", sizeof(int));
  printf("%d\n", sizeof(double));
  printf("%d\n", sizeof(long));
  printf("%lu\n", sizeof(float));

  return 0;
}
